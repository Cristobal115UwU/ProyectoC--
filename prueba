int x;
int y;

struct {
    int x;
    float y;
} a;

struct b {
    float x;
} c;

int suma ( int a , int b ) {
    int c;
    c = a + b;
    return c;
}

void main(){
    x = 6;
    y = 1;
    x = suma(x, y);
    while(x > 0){
        y = y * x;
        x = x - 1;
    }

    print("El resultado es: \n\t");
    print("\n");
    print("\t");
    print("El resultado de la x + 5 es: \n\t");
    print(x + 5); 
    print(" ");
    cadena1 = "a";
    cadena2 = 'Mundo';
}
